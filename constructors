class Candy {

       private String maker;	//e.g. Hershey’s, Willy Wonka
       private boolean isChocolatey;
       private double price;
       
       
       
       Candy(String m, boolean ic, double p){     
this.maker=m;
this.isChocolatey=ic;
this.price=p;

       
       }
     Candy.  
       

}






